{
  "textures": {
    "particle": "etherology:block/ethereal_channel"
  },
  "multipart": [
    {
      "apply": { "model": "etherology:block/ethereal_channel_input"},
      "when": { "north": "in" }
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_output"},
      "when": { "north": "out" }
    },

    {
      "apply": { "model": "etherology:block/ethereal_channel_input", "y": 90 },
      "when": { "east": "in" }
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_output", "y": 90 },
      "when": { "east": "out" }
    },

    {
      "apply": { "model": "etherology:block/ethereal_channel_input", "y": 180 },
      "when": { "south": "in" }
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_output", "y": 180 },
      "when": { "south": "out" }
    },

    {
      "apply": { "model": "etherology:block/ethereal_channel_input", "y": 270 },
      "when": { "west": "in" }
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_output", "y": 270 },
      "when": { "west": "out" }
    },

    {
      "apply": { "model": "etherology:block/ethereal_channel_input", "x": 270 },
      "when": { "up": "in" }
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_output", "x": 270 },
      "when": { "up": "out" }
    },

    {
      "apply": { "model": "etherology:block/ethereal_channel_input", "x": 90 },
      "when": { "down": "in" }
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_output", "x": 90 },
      "when": { "down": "out" }
    },


    {
      "apply": { "model": "etherology:block/ethereal_channel_central_line" },
      "when": { "OR": [
        { "facing": "north", "is_cross": "false" },
        { "facing": "south", "is_cross": "false" }
      ]}
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_central_line", "y": 90 },
      "when": { "OR": [
        { "facing": "east", "is_cross": "false" },
        { "facing": "west", "is_cross": "false" }
      ]}
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_central_line", "x": 90 },
      "when": { "OR": [
        { "facing": "down", "is_cross": "false" },
        { "facing": "up", "is_cross": "false" }
      ]}
    },

    {
      "apply": { "model": "etherology:block/ethereal_channel_central_cross" },
      "when": { "OR": [
        { "facing": "north", "is_cross": "true" },
        { "facing": "south", "is_cross": "true" }
      ]}
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_central_cross", "y": 90 },
      "when": { "OR": [
        { "facing": "east", "is_cross": "true" },
        { "facing": "west", "is_cross": "true" }
      ]}
    },
    {
      "apply": { "model": "etherology:block/ethereal_channel_central_cross", "x": 90 },
      "when": { "OR": [
        { "facing": "down", "is_cross": "true" },
        { "facing": "up", "is_cross": "true" }
      ]}
    }
  ]
}